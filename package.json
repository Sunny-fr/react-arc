{
  "name": "react-arc",
  "version": "4.0.5",
  "description": "React Abstract Redux Component",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepare": "npm run compile",
    "prepublish-and-move-to-demo": "npm run compile && cp -r lib demo/src",
    "watch": "gulp watch",
    "test": "ava"
  },
  "prettier": {
    "semi": false
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.{js,es}",
      "!test/__lib__/**/*"
    ],
    "source": [
      "**/*.{js,jsx,es}",
      "!build/**/*"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-fr/react-arc.git"
  },
  "author": "Alaa El Helw",
  "license": "ISC",
  "keywords": [
    "react",
    "redux"
  ],
  "bugs": {
    "url": "https://github.com/Sunny-fr/react-arc/issues"
  },
  "homepage": "https://github.com/Sunny-fr/react-arc#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-watch": "^4.3.9",
    "nyc": "^8.1.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "deep-equal": "^1.1.1",
    "lodash.clonedeep": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  }
}
