{
  "name": "react-arc",
  "version": "1.0.1",
  "description": "React Abstract Redux Component",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-0 -d lib/ src/",
    "prepublish": "npm run compile",
    "watch": "gulp watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.{js,es}",
      "!test/__lib__/**/*"
    ],
    "source": [
      "**/*.{js,jsx,es}",
      "!build/**/*"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-fr/react-arc.git"
  },
  "author": "Alaa El Helw",
  "license": "ISC",
  "keywords": [
    "react",
    "redux"
  ],
  "bugs": {
    "url": "https://github.com/Sunny-fr/react-arc/issues"
  },
  "homepage": "https://github.com/Sunny-fr/react-arc#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-watch": "^4.3.9",
    "nyc": "^8.1.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "deep-equal": "^1.0.1",
    "react": "^15.4.2"
  }
}
